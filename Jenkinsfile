pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'Building the project...'
            }
        }
        stage('Test') {
            steps {
                echo 'Running tests...'
            }
        }
      stage('Generating artifacts') {
            steps {
                echo 'Generating artifacts...'
            }
        }
      stage('Release') {
            steps {
                echo 'Releasing the project...'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying the project...'
            }
        }
    }
}
